square = (x) -> x * x
double = (x) -> 2 * x


val = 123

puts(val)
puts(double(val))
puts(square(val))
puts(double(square(val)))
puts(square(double(val)))

make_say(x) {
  puts(4)
  puts(x)
  f0 = () -> {
    puts(3)
    puts(x)
  }
  puts(f0)
  f0
}

twice(f) {
  puts(f, 1)
  f()
  puts(f, 2)
  f()
}

twice(make_say(1234))
